{
	"name": "document",
	"description": "Document template used for testing.",
	"output": "html",
	"dependencies": [
	],
	"tags": {
		"*": "<h1><code>${content}</code></h1>",
		"document": "<!doctype html>\n<html>\n<head>\n<meta charset='UTF-8'>\n<title>${title}</title>\n<meta name='author' content='${author}'/>\n<meta name='date' content='${date}'/>\n<link rel='stylesheet' type='text/css' href='${template-path}/${class}.css'>\n</head>\n<body>\n<header><h1>${title}</h1></header>\n${content}</body>\n</html>\n",
		"heading": "<h${level}>${content}</h${level}>\n",
		"paragraph": "<p>${content}</p>\n",
		"list item": "<li>${content}</li>\n",
		"unordered list": "<ul>\n${content}</ul>\n",
		"ordered list": "<ol>\n${content}</ol>\n",
		"emphasize": "<em>${content}</em>",
		"link": "<a href='${target}'>${content}</a>"
	}
}
