{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "pdf",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/build/typeup.js",
			"stopOnEntry": false,
			"args": ["pdf", "./examples"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/build"
		},
