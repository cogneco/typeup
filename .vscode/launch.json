{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "pdf",
			"program": "${workspaceRoot}/build/app.js",
			"args": ["pdf", "./docs/document.tup"],
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "html",
			"program": "${workspaceRoot}/build/app.js",
			"args": ["html", "./docs/document.tup"],
			"console": "internalConsole",
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "json",
			"program": "${workspaceRoot}/build/app.js",
			"args": ["json", "./docs/document.tup"],
			"console": "internalConsole",
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "self-test",
			"program": "${workspaceRoot}/build/app.js",
			"stopOnEntry": false,
			"args": ["self-test"],
			"console": "internalConsole",
			"protocol": "inspector",
			"preLaunchTask": "build",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/build/**/*.js" ]
		}
	]
}
